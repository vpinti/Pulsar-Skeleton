{
    "name": "pulsar/pulsar",
    "description": "The skeleton application for the Pulsar framework.",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "framework"
        }
    ],
    "require-dev": {
        "symfony/var-dumper": "6.3.x-dev"
    },
    "require": {
        "pulsar/framework": "dev-main"
    }
}
